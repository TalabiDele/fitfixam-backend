{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "posts",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "post": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "post"
    },
    "user": {
      "via": "posts",
      "plugin": "users-permissions",
      "model": "user"
    },
    "likes": {
      "via": "post",
      "collection": "likes"
    },
    "comments": {
      "via": "post",
      "collection": "comments"
    },
    "category": {
      "model": "categories",
      "via": "posts"
    },
    "images": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
